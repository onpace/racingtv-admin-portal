{
  "name": "admin-portal",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aws-sdk/client-ecs": "^3.36.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "express": "^4.17.1",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.1.2",
    "wait-on": "^6.0.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start:api": "cd src/api/ && ts-node-dev --respawn . && wait-on tcp:5000",
    "start:app": "react-scripts start",
    "start": "run-p --race start:api start:app",
    "build:app": "react-scripts build",
    "build:api": "npx tsc src/api/index.ts --outDir build_api",
    "build": "run-p build:app build:api",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "nodemon ./src/Api.tsx"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.13",
    "ts-node": "^10.2.1"
  },
  "proxy": "http://localhost:8080"
}
